<h1>Editing PhotoInfo</h1>

<%= form_for(@travel_photo) do |f| %>
  <div class="form-group">
    <%= f.label :photo_service_album_id %><br>
    <%= f.select :photo_service_album_id, @album_list, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :name %><br>
    <%= f.text_field :name, class: "form-control" %>
  </div>
  <%= f.hidden_field :travel_id %>
  <%= f.hidden_field :photo_service_user_info_id %>
  <%= f.submit class: "btn btn-default", value: '登録' %>
<% end %>
